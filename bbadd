#!/bin/bash
let "W=32"
let "H=18"
F=60
WW=800
HH=450
PID="$(pidof mpv)"
let "R = $W * $H * $F "
echo "calculated rate: $R"
FORMAT=y8
S="$1"
(echo "main(t){for(t=0;;t++){int w=$W;int h=$H;int x=t%w;int y=(t/w);int f=((int)(t/(w*h)));if(y>=h){y=0;};putchar($S);}}" | gcc -x c - -lm -o /tmp/bytebeat && \
  (/tmp/bytebeat >> /tmp/bytes)&
)
