echo "main(t){for(t=0;;t++)putchar(((t*t+t)>>t/5000)|t*t);}" | gcc -x c - -lm -o /tmp/bytebeat && /tmp/bytebeat | ffmpeg -f rawvideo -pix_fmt gray -s 64x36 -i - -f s16le -ar 8000 -ac 2 -i - -vf "[in]fade=in:0:50[a],[a]fade=out:3300:300[b],[b]scale=1280:720[out]" -af "[in]afade=t=in:ss=0:d=2[a],[a]afade=t=out:st=110:d=10[out]" -c:v libx264 -b:v 6M -r 30 -c:a aac -strict -2 -ar 44100 -b:a 128k -t 120 -y ~/render/byteBeat.mp4
